pipeline {
    agent any

    tools {
        maven "maven38"
    }

    stages {
        stage('SCM Checkout') {
            steps {
                // Get code from a GitHub repository
                git 'https://github.com/neocolss/DemoTokenAPI.git'

                // Run Maven on a Windows agent
                sh "mvn clean test -Dtest=ExamplesTest"
            }
        }
    }
}
